{
  "name": "tp5",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:prod": "ng serve --configuration production",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "format": "prettier --write \"src/**/*.{ts,html,css,less,scss,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,html,css,less,scss,json}\"",
    "format:all": "prettier --write \"**/*.{ts,html,css,less,scss,json,md}\" --ignore-path .prettierignore",
    "check": "npm run format:check && npm run lint",
    "fix": "npm run format && npm run lint:fix",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{html,css,less,scss}": [
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^20.3.11",
    "@angular/compiler": "^20.3.15",
    "@angular/core": "^20.3.11",
    "@angular/forms": "^20.3.11",
    "@angular/platform-browser": "^20.3.11",
    "@angular/router": "^20.3.11",
    "@ngxs/storage-plugin": "^20.1.0",
    "@ngxs/store": "^20.1.0",
    "rxjs": "~7.8.2",
    "tslib": "^2.8.1",
    "zone.js": "~0.15.1"
  },
  "devDependencies": {
    "@angular/build": "^20.3.10",
    "@angular/cli": "^20.3.13",
    "@angular/compiler-cli": "^20.3.11",
    "@types/jasmine": "~5.1.12",
    "angular-eslint": "20.6.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "jasmine-core": "~5.12.1",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "less": "^4.4.2",
    "lint-staged": "^16.2.6",
    "prettier": "3.6.2",
    "typescript": "~5.9.3",
    "typescript-eslint": "8.46.4"
  }
}
